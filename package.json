{
  "name": "plezanje-graphql",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:show": "npm run typeorm migration:show"
  },
  "dependencies": {
    "@apollo/gateway": "^0.42.0",
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.3",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-fastify": "^8.4.1",
    "@nestjs/typeorm": "^8.0.2",
    "@types/nodemailer": "^6.4.4",
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-core": "^3.6.4",
    "apollo-server-fastify": "^3.6.4",
    "apollo-server-plugin-response-cache": "^3.5.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.6.0",
    "fs": "0.0.1-security",
    "graphql": "^15.7.2",
    "graphql-tools": "^8.2.0",
    "handlebars": "^4.7.7",
    "hbs": "^4.2.0",
    "ioredis": "^4.28.5",
    "mysql": "^2.14.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "postgres": "^1.0.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sharp": "^0.30.3",
    "slugify": "^1.6.3",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.3",
    "@types/bcrypt": "^3.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^8.0.29",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.3",
    "fast-xml-parser": "^4.0.7",
    "jest": "^27.0.7",
    "prettier": "^1.19.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^6.2.1",
    "ts-morph": "^13.0.2",
    "ts-node": "3.3.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
